<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/nav_field_book_map">
    <fragment
        android:id="@+id/nav_field_book_map"
        android:name="berlin.mfn.naturblick.ui.fieldbook.fieldbookmap.FieldbookMapFragment"
        android:label="@string/field_book_map"
        tools:layout="@layout/fragment_fieldbook_map">
        <argument
            android:name="occurence_id"
            app:argType="android.os.ParcelUuid"
            app:nullable="true"
            android:defaultValue="@null"
            />
        <action
            android:id="@+id/nav_field_book_map_to_nav_field_book_observation"
            app:destination="@id/nav_field_book_observation" />
    </fragment>
    <activity
        android:id="@+id/nav_field_book_observation"
        android:name="berlin.mfn.naturblick.ui.fieldbook.observation.ObservationActivity"
        android:label="@string/field_book_observation">
        <argument
            android:name="observation_action"
            app:argType="berlin.mfn.naturblick.ui.fieldbook.ObservationAction" />
    </activity>
</navigation>
